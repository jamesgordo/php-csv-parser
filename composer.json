{
    "name": "jamesgordo/php-csv-parser",
    "version": "1.0.6",
    "description": "Easiest and Convenient Way to Parse CSV Files using PHP.",
    "type": "library",
    "keywords": ["php", "csv parser", "php csv parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Gordo"
        }
